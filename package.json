  "scripts": {
    "format": "prettier --write --ignore-unknown .",
    "lint": "eslint --ext .ts,.tsx --fix .",
    "test": "jest --coverage=false --detectOpenHandles --maxWorkers=1",
    "typecheck": "tsc --noEmit",
    "taxfy": "run-p taxfy:*",
    "taxfy:taxfy": "pnpm -F taxfy-func dev",
    "taxfy-func": "pnpm -F taxfy-func",
    "webapp": "pnpm -F webapp",
    "taxfy:dev": "firebase emulators:start --import=./tmp/data --export-on-exit",
    "taxfy:solanamobile": "pnpm android",
    "taxfy:webapp": "pnpm -F webapp dev",
    "dev": "EXPO_USE_PATH_ALIASES=1 expo start -c",
    "android": "ANDROID_HOME=~/Library/Android/sdk npx expo run:android",
    "update:packages": "ncu -x 'react-native-reanimated,react-native-svg' && pnpm",
    "build:configure": "eas build:configure",
    "build:development:android": "eas build --platform android --profile development --local",
    "build:preview:android": "eas build --platform android --profile preview --local",
    "build:production:android": "eas build --platform android --profile production --local",
    "build:sms": "pnpm build:development:android",
    "build": "pnpm build:production:webapp",
    "deploy": "pnpm deploy:webapp",
    "build:production:webapp": "pnpm -F webapp build",
    "deploy:rules": "npx firebase deploy --only firestore:rules,storage",
    "deploy:webapp": "pnpm build:production:webapp && npx firebase deploy --only hosting",
    "start": "expo start --dev-client",
    "ios": "expo run:ios",
    "solana:key:convert": "npx ts-node -r tsconfig-paths/register --transpile-only scripts/solana/privateKeyConverter.ts",
    "solana:nft:create:collection": "npx ts-node -r tsconfig-paths/register --transpile-only scripts/solana/cNFT/createNFTCollection.ts",
    "solana:nft:create:merkleTree": "npx ts-node -r tsconfig-paths/register --transpile-only scripts/solana/cNFT/createMerkleTree.ts",
    "solana:nft:mint:cnft": "npx ts-node -r tsconfig-paths/register --transpile-only scripts/solana/cNFT/mintCNFT.ts",
    "solana:nft:get:cnft": "npx ts-node -r tsconfig-paths/register --transpile-only scripts/solana/cNFT/getCNFT.ts",
    "discord": "npx ts-node -r tsconfig-paths/register --transpile-only ./.github/workflows/runDiscordChangeLog.ts"
  },

{
    "app": {
        "name": "taxfy",
        "projectId": "taxfy",
        "fbProjectId": "taxifyio",
        "region": "nam5",
        "template": "Taxfy Mobile Stack (Expo) + Web (Next.js) - Firestore",
        "appDomain": "taxfy.io",
        "lbDomain": "taxfy.io",
        "secretKeys": [],
        "hasLoadBalancer": false
    },
    "cloudArmor": [
        {
            "securityPolicyName": "taxfy-taxfy-armor",
            "rules": [
                {
                    "priority": "10",
                    "description": "Allow Your Home IP addresses",
                    "options": {
                        "src-ip-ranges": "x.x.x.x",
                        "action": "allow"
                    }
                },
                {
                    "priority": "20",
                    "description": "Allow Firebase Function calls",
                    "options": {
                        "src-ip-ranges": "0.0.0.0/0",
                        "action": "allow"
                    }
                },
                {
                    "priority": "300",
                    "description": "Defense from NodeJS attack",
                    "options": {
                        "action": "deny-403",
                        "expression": "evaluatePreconfiguredExpr('nodejs-v33-stable')"
                    }
                }
            ]
        }
    ]
}